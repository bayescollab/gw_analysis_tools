  RowBox[{
   RowBox[{"Y", "[", 
    RowBox[{"s_", ",", "l_", ",", "m_", ",", "\[Theta]_", ",", "\[Phi]_"}], 
    "]"}], ":=", 
   RowBox[{
    SuperscriptBox[
     RowBox[{"(", 
      RowBox[{"-", "1"}], ")"}], "m"], 
    RowBox[{"Simplify", "[", 
     RowBox[{
      RowBox[{
       SqrtBox[
        FractionBox[
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"l", "+", "m"}], ")"}], "!"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"l", "-", "m"}], ")"}], "!"}], 
          RowBox[{"(", 
           RowBox[{
            RowBox[{"2", "l"}], "+", "1"}], ")"}]}], 
         RowBox[{
          RowBox[{
           RowBox[{"(", 
            RowBox[{"l", "+", "s"}], ")"}], "!"}], 
          RowBox[{
           RowBox[{"(", 
            RowBox[{"l", "-", "s"}], ")"}], "!"}], "4", "\[Pi]"}]]], 
       SuperscriptBox[
        RowBox[{"(", 
         RowBox[{"Sin", "[", 
          FractionBox["\[Theta]", "2"], "]"}], ")"}], 
        RowBox[{"2", "l"}]], 
       RowBox[{
        SubsuperscriptBox["\[Sum]", 
         RowBox[{"r", "=", "0"}], 
         RowBox[{"l", "-", "s"}]], 
        RowBox[{"(", 
         RowBox[{
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{"l", "-", "s"}], ",", "r"}], "]"}], 
          RowBox[{"Binomial", "[", 
           RowBox[{
            RowBox[{"l", "+", "s"}], ",", 
            RowBox[{"r", "+", "s", "-", "m"}]}], "]"}], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"-", "1"}], ")"}], 
           RowBox[{"l", "-", "r", "-", "s"}]], 
          SuperscriptBox["\[ExponentialE]", 
           RowBox[{"\[ImaginaryI]", " ", "m", " ", "\[Phi]"}]], 
          SuperscriptBox[
           RowBox[{"(", 
            RowBox[{"Cot", "[", 
             FractionBox["\[Theta]", "2"], "]"}], ")"}], 
           RowBox[{
            RowBox[{"2", "r"}], "+", "s", "-", "m"}]]}], ")"}]}]}], ",", 
      RowBox[{"Assumptions", "\[Rule]", 
       RowBox[{"{", 
        RowBox[{
         RowBox[{"\[Phi]", "\[Element]", "Reals"}], ",", 
